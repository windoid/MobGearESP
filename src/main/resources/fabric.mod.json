  "name": "Mobgear Addon",
  "description": "A module to detect if mobs are wearing player gear",
  "authors": [
    "windoid"
  ],
  "contact": {
    "repo": "https://github.com/windoid/MobGearESP"
  },
  "icon": "assets/template/icon.png",
